<Window x:Class="auto_loadless.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:auto_loadless"
        mc:Ignorable="d"
        Title="Auto Loadless" Height="720" Width="1280">
    <Grid x:Name="gridMain">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="gridRight" Grid.Column="1" FlowDirection="LeftToRight" Margin="0,0,0,-1">
            <Grid.RowDefinitions>
                <RowDefinition Height="100*"/>
                <RowDefinition Height="100"/>
            </Grid.RowDefinitions>
            <Image x:Name="image" Margin="10,10,10,10" Source="/chrome_7fp5ib9rjN.png" StretchDirection="Both" Stretch="Uniform"/>
            <Slider x:Name="slider" Margin="10,0,10,0" VerticalAlignment="Top" Grid.Row="1" Height="20"/>
            <Grid x:Name="gridVideoControls" Grid.Row="1" Margin="0,23,0,46">
                <Grid.RowDefinitions>
                    <RowDefinition Height="20*"/>
                    <RowDefinition Height="9*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="7*"/>
                    <ColumnDefinition Width="3*"/>
                </Grid.ColumnDefinitions>
                <Button x:Name="button" Content="&lt;&lt;" Margin="10,5,5,5" Grid.RowSpan="2" VerticalAlignment="Top"/>
                <Button x:Name="button_Copy" Content="&lt;" Margin="5,5,5,5" Grid.Column="1" Grid.RowSpan="2"/>
                <Button x:Name="button_Copy1" Content="&gt;" Margin="5,5,5,5" Grid.Column="2" Grid.RowSpan="2"/>
                <Button x:Name="button_Copy2" Content="&gt;&gt;" Margin="5,5,5,5" Grid.Column="3" Grid.RowSpan="2"/>
            </Grid>
            <Label x:Name="label" Content="Status..." Margin="10,69,10,0" Grid.Row="1" VerticalAlignment="Top" Height="21" Padding="0,0,0,0"/>
        </Grid>
        <Grid x:Name="gridLeft" Margin="0,0,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Rectangle Stroke="Black" Margin="10,10,10,10"/>
        </Grid>

    </Grid>
</Window>
